93f\u0924\u093e\u0915\u093e \u0909\u0924\u094d\u0915\u0943\u0937\u094d\u091f \u0968\u0969 \u092a\u094d\u0930\u0924\u093f\u0938\u094d\u092a\u0930\u094d\u0927\u0940 \u0938\u093e\u0930\u094d\u0935\u091c\u0928\u093f\u0915 \u0917\u0930\u0947\u0915\u094b \u0939\u094b\u0964<\/p>\n<p>\u0938\u094d\u092f\u093e\u0938\u093f\u0919 \u0938\u092e\u093e\u0930\u094b\u0939\u092e\u093e \u0909\u0924\u094d\u0915\u0943\u0937\u094d\u091f \u0968\u0969 \u0932\u093e\u0908 \u0915\u0928\u094d\u091f\u0947\u0938\u094d\u091f\u0947\u0928\u094d\u091f \u091f\u094d\u092f\u093e\u0917 \u0928\u092e\u094d\u092c\u0930 \u092a\u094d\u0930\u0926\u093e\u0928 \u0917\u0930\u093f\u090f\u0915\u094b \u091b\u0964 \u092a\u094d\u0930\u0924\u093f\u092f\u094b\u0917\u093f\u0924\u093e\u0915\u094b \u0905\u0928\u094d\u0924\u093f\u092e \u0938\u094d\u092a\u0930\u094d\u0927\u093e\u092e\u093e \u092a\u0935\u093f\u0928\u093e \u0936\u094d\u0930\u0947\u0937\u094d\u0920, \u0938\u0940\u0924\u093e \u0938\u0941\u092c\u0947\u0926\u0940, \u0938\u0941\u0938\u094d\u092e\u093f\u0924\u093e \u0938\u093f\u0902\u0939, \u0928\u093f\u0936\u093e \u0935\u093f\u0937\u094d\u091f, \u0938\u0932\u094b\u0928\u0940 \u0936\u094d\u0930\u0947\u0937\u094d\u0920, \u092a\u0941\u0928\u092e \u0936\u094d\u0930\u0947\u0937\u094d\u0920, \u0906\u0907\u0928\u093e \u0917\u091c\u0941\u0930\u0947\u0932, \u0926\u0947\u0935\u0940 \u0925\u093e\u092a\u093e \u0917\u0941\u0930\u0941\u0919, \u092a\u0943\u0925\u094d\u0935\u0940\u092f\u093e \u0925\u093e\u092a\u093e, \u092e\u0940\u0928\u093e \u092d\u091f\u094d\u091f\u0930\u093e\u0908, \u0906\u092f\u0941\u0937\u094d\u092e\u093e \u0930\u093e\u0908, \u0930\u0947\u0916\u093e \u0935\u093f\u0937\u094d\u091f, \u091c\u094d\u091e\u093e\u0928\u0947\u0936\u094d\u0935\u0930\u0940 \u0936\u094d\u0930\u0947\u0937\u094d\u0920, \u0938\u093f\u0930\u094d\u091c\u0928\u093e \u0915\u0947\u0938\u0940, \u091f\u0947\u0915\u0915\u0941\u092e\u093e\u0930\u0940 \u0918\u0932\u0947, \u092e\u0902\u0917\u0932\u0940 \u0938\u093f\u0932\u0935\u093e\u0932, \u0915\u0943\u0924\u093f\u0915\u093e \u092a\u093e\u0932\u093f\u0916\u0947, \u0915\u094b\u092e\u0932 \u0932\u093e\u092e\u093e, \u0930\u0947\u0916\u093e \u0938\u093f\u0902\u0939 \u0930\u093e\u092f\u092e\u093e\u091d\u0940, \u0928\u093f\u0930\u094d\u092e\u0932\u093e \u0917\u0941\u0930\u0941\u0919, \u0909\u0924\u094d\u0938\u0941\u0915\u094d\u0924\u093e \u0928\u0915\u0930\u094d\u092e\u0940, \u091c\u0947\u0928\u0940 \u0916\u0921\u094d\u0915\u093e \u0930 \u0938\u0930\u0941 \u0906\u0930 \u090f\u0932 \u0930\u093e\u0923\u093e \u0938\u0939\u092d\u093e\u0917\u0940 \u091b\u0928\u094d\u0964 \u092a\u094d\u0930\u0924\u093f\u0938\u094d\u092a\u0930\u094d\u0927\u0940\u0939\u0930\u0942\u0932\u0947 \u0906\u092b\u094d\u0928\u094b \u092a\u0930\u093f\u091a\u092f \u0926\u093f\u201c\u0926\u0948 \u092a\u094d\u0930\u0924\u093f\u092f\u094b\u0917\u093f\u0924\u093e\u092e\u093e \u092d\u093e\u0917 \u0932\u093f\u0928\u0941\u0915\u094b \u0909\u0926\u094d\u0926\u0947\u0936\u094d\u092f\u092c\u093e\u0930\u0947 \u092a\u094d\u0930\u0937\u094d\u091f \u092a\u093e\u0930\u0947\u0915\u093e \u0925\u093f\u090f\u0964<\/p>\n<p>\u092a\u094d\u0930\u0924\u093f\u0938\u094d\u092a\u0930\u094d\u0927\u0940\u0939\u0930\u0942\u0932\u0947 \u0935\u093f\u092d\u093f\u0928\u094d\u0928 \u091a\u0930\u0923 \u092a\u093e\u0930 \u0917\u0930\u094d\u0926\u0948 \u0906\u0917\u093e\u092e\u0940 \u0915\u093e\u0924\u094d\u0924\u093f\u0915 \u0968 \u0917\u0924\u0947 \u0930\u093e\u091c\u0927\u093e\u0928\u0940\u092e\u093e \u0939\u0941\u0928\u0947 \u092a\u094d\u0930\u0924\u093f\u092f\u094b\u0917\u093f\u0924\u093e\u0915\u094b \u0905\u0928\u094d\u0924\u093f\u092e \u092a\u094d\u0930\u0924\u093f\u0938\u094d\u092a\u0930\u094d\u0927\u093e\u092e\u093e \u0924\u093e\u091c\u0915\u093e \u0932\u093e\u0917\u093f \u0938\u094d\u092a\u0930\u094d\u0927\u093e \u0917\u0930\u094d\u0928\u0947\u091b\u0928\u094d\u0964 \u0906\u092f\u094b\u091c\u0915\u0915\u093e \u0905\u0928\u0941\u0938\u093e\u0930 \u0909\u0924\u094d\u0915\u0943\u0937\u094d\u091f \u0968\u0969 \u092a\u094d\u0930\u0924\u093f\u0938\u094d\u092a\u0930\u094d\u0927\u0940\u0932\u093e\u0908 \u0935\u094d\u092f\u0915\u094d\u0924\u093f\u0924\u094d\u0935 \u0935\u093f\u0915\u093e\u0938\u0938\u092e\u094d\u092c\u0928\u094d\u0927\u0940 \u092a\u094d\u0930\u0936\u093f\u0915\u094d\u0937\u0923\u092e\u093e \u0938\u0939\u092d\u093e\u0917\u0940 \u0917\u0930\u093e\u0907\u0928\u0947\u091b\u0964 \u0905\u0928\u094d\u0924\u093f\u092e \u092a\u094d\u0930\u0924\u093f\u0938\u094d\u092a\u0930\u094d\u0927\u093e\u092e\u093e \u0915\u094b\u0930\u093f\u092f\u094b\u0917\u094d\u0930\u093e\u092b\u0930 \u0905\u0936\u094d\u0935\u093f\u0928\u0940\u0915\u093e\u0928\u094d\u0924 \u091d\u093e\u0932\u0947 \u0915\u094b\u0930\u093f\u092f\u094b\u0917\u094d\u0930\u093e\u092b\u0940 \u0917\u0930\u094d\u0928\u0947\u091b\u0928\u094d\u0964<\/p>\n<p>\u0905\u0928\u094d\u0924\u093f\u092e \u092a\u094d\u0930\u0924\u093f\u0938\u094d\u092a\u0930\u094d\u0927\u093e\u0915\u094b \u0938\u092e\u093e\u0930\u094b\u0939\u092e\u093e \u092e\u093f\u0938\u0947\u0938 \u0928\u0947\u092a\u093e\u0932 \u0935\u0932\u094d\u0930\u094d\u0921\u2013\u0968\u0966\u0968\u0969 \u0915\u093e \u0905\u0928\u094d\u092f \u0909\u092a\u0909\u092a\u093e\u0927\u093f \u0935\u093f\u091c\u0947\u0924\u093e\u0915\u094b \u092a\u0928\u093f \u0918\u094b\u0937\u0923\u093e \u0917\u0930\u093f\u0928\u0947 \u0906\u092f\u094b\u091c\u0915 \u0938\u0902\u0938\u094d\u0925\u093e\u0915\u093e \u0905\u0927\u094d\u092f\u0915\u094d\u0937 \u0928\u093f\u0936\u0941\u0915\u093e\u0928\u094d\u0924 \u091d\u093e\u0932\u0947 \u091c\u093e\u0928\u0915\u093e\u0930\u0940 \u0917\u0930\u093e\u090f\u0964 \u092e\u093f\u0938\u0947\u0938 \u0928\u0947\u092a\u093e\u0932 \u0935\u0932\u094d\u0930\u094d\u0921\u2013\u0968\u0966\u0968\u0969 \u0915\u094b \u0935\u093f\u091c\u0947\u0924\u093e\u0932\u0947 \u0906\u0917\u093e\u092e\u0940 \u0921\u093f\u0938\u0947\u092e\u094d\u092c\u0930\u092e\u093e \u0905\u092e\u0947\u0930\u093f\u0915\u093e\u092e\u093e \u0939\u0941\u0928\u0947 \u092e\u093f\u0938\u0947\u0938 \u0935\u0932\u094d\u0930\u0